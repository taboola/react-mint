{
  "name": "react-mint",
  "version": "1.0.0",
  "description": "Clean React tooltips utilizing built-in React sugar like portals and context",
  "main": "./dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base demo/public/ --config webpack.dev.js --display-error-details",
    "build": "babel src -d dist",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "react",
    "tooltip"
  ],
  "author": "Kevin Zuo",
  "license": "Apache-2.0",
  "repository": "https://github.com/taboola/react-mint",
  "peerDependencies": {
    "react": ">= 16.3 < 17",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "memoize-one": "^4.0.2",
    "prop-types": "^15.6.2",
    "shallowequal": "^1.1.0",
    "uuid": "^3.3.2"
  }
}
